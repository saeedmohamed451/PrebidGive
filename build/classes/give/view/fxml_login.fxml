<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>


<VBox fx:id="mainPane" alignment="BOTTOM_CENTER" prefHeight="355.0" prefWidth="520.0" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="give.core.controller.LoginController">
   <stylesheets>
        <URL value="@login.css" />
    </stylesheets>
    <children>
        
<!--      <Label fx:id="welcometext" alignment="CENTER" prefHeight="92.0" prefWidth="381.0" text="Welcome to Give"/>-->

      <GridPane fx:id="gridPane" alignment="BOTTOM_CENTER" gridLinesVisible="false" hgap="23.0" prefHeight="70.0" prefWidth="226.0" vgap="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="290.0" minWidth="10.0" prefWidth="82.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="448.0" minWidth="10.0" prefWidth="308.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="40.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="40.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         
             
            
            <TextField fx:id="txtUserEmail" text="test@test.com" promptText="email" prefHeight="50.0" maxWidth="200.0" GridPane.columnIndex="1" />
            <PasswordField fx:id="txtPassword" text="123123123" promptText="password" prefHeight="50.0" maxWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         
      </GridPane>
      <Hyperlink onAction="#onForgotPressed" text="forgot password" fx:id="forgotButton" prefWidth="199"/>
      <Button mnemonicParsing="false" fx:id="loginButton" onAction="#loginButtonPressed" prefHeight="24.0" prefWidth="70.0" text="Log In" />
      <Hyperlink onAction="#onCreateAccountPressed" fx:id="createAccount" prefHeight="24.0" prefWidth="140.0" text="Create an account" alignment="CENTER" />
      
      
      
   </children>
   <padding>
      <Insets bottom="70.0" left="100.0" right="100.0" top="70.0" />
   </padding>
</VBox>
