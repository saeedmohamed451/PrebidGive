<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="give.core.controller.HomeController">
    <stylesheets>
        <URL value="@home.css" />
    </stylesheets>
   <children>
      <Label fx:id="lblUserName" alignment="TOP_RIGHT" layoutX="356.0" layoutY="16.0" prefHeight="23.0" prefWidth="183.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="2.0" layoutY="77.0" prefHeight="223.0" prefWidth="594.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#giveButtonPressed" prefHeight="34.0" prefWidth="85.0" text="Give" />
            <Button fx:id="btnCharity" mnemonicParsing="false" onAction="#charityButtonPressed" prefHeight="34.0" prefWidth="85.0" text="Charities" />
            <Button mnemonicParsing="false" onAction="#accountButtonPressed" prefHeight="34.0" prefWidth="85.0" text="Account" />
         </children>
      </VBox>
      <Hyperlink fx:id="linkLogout" layoutX="540.0" layoutY="16.0" onAction="#onLinkLogoutPressed" text="Logout" />
   </children>
</AnchorPane>
